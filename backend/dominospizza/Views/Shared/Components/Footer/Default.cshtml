@model List<Contact>

<footer>
    <div class="container ">
        <div class="row ">
            <div class="col-lg-12 col-md-12 ">
                <ul class="img-list ">
                    @foreach (Contact item in Model)
                    {
                        <li>
                            <img src="~/img/footer/@item.FooterLogo" alt="That's an image ">
                        </li>
                    }
                </ul>
            </div>
        </div>
        <div class="row ">
            <div class="col-lg-12 col-md-12 ">
                <ul class="link-list ">
                    <li>
                        <a asp-controller="About">Domino's Haqqında</a>
                        <span class="right-line "></span>
                    </li>

                    <li>
                        <a asp-controller="Contact" asp-action="Index">Əlaqə</a>
                    </li>
                </ul>

            </div>
        </div>
        <div class="row ">
            <div class="lists ">
                <ul class="store-list ">
                    <li>
                        <a href="https://play.google.com/store/apps/details?id=com.dominospizza.az "><img src="~/img/footer/google-play.png" style="height: 47px; " alt="That's an image "></a>
                    </li>
                    <li>
                        <a href="https://apps.apple.com/us/app/dominos-pizza-azerbaijan/id1535012493 "> <img src="~/img/footer/App-store.png" alt="That's an image "></a>
                    </li>

                    <li> <span class="right-line "></span></li>
                </ul>

                <ul class="social-list ">
                    @foreach (Contact item in Model)
                    {
                        <li>
                            <a href="@item.SocialLink"> <img src="~/img/footer/@item.SocialImg" alt="That's an image "></a>
                        </li>
                    }

                    <li>
                        <span class="right-line "></span>
                    </li>
                </ul>

                <ul class="pay-list ">
                    @foreach (Contact item in Model)
                    {
                        <li>
                            <a><img src="~/img/footer/@item.PayImg" alt="That's an image "></a>
                        </li>
                    }
                </ul>
            </div>
        </div>
       @await Component.InvokeAsync("Copyright")
    </div>

</footer>