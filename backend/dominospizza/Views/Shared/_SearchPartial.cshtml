
@model List<Product>


@foreach (Product product in Model)
{
    <li class="list-group-item" style="margin-top: 5px;text-align:center; font-size: 20px; border-bottom: 1px solid #d9d9d9; padding: 10px 0;">
        <a asp-controller="product" asp-action="detail" asp-route-id="@product.Id">@product.Name</a>
    </li>
}